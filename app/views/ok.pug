extends scaffold

block content
  div(class="mt-5")
    p(style="font-size:50px") &#x2714
    if (typeof(messages) !== 'undefined')
        div(class="row align-items-center h-100")
          div(class="col-md-6 mx-auto")
            div(class="mb-4")
              for msg in messages
                div(class="alert alert-success") #{ msg }
    button(class="btn btn-primary btn-lg entry button my-3" onclick=`location.href='${back_url !== undefined ? back_url : "/"}'` ) Back
