extends scaffold_dev

block content
  div(class="mt-5")
    p(style="font-size:50px") &#x26A0
    if (typeof(messages) !== 'undefined')
        div(class="row align-items-center h-100")
          div(class="col-md-6 mx-auto")
            div(class="mb-4")
              for msg in messages
                div(class="alert alert-danger") #{ msg }
    button(class="btn btn-primary btn-lg entry button my-3" onclick="location.href='/'" ) Back
